<% include /partials/header %>

<div class="container">
	<div class="row">
	<h1 style="text-align: center">Add New Ramen Location</h1>
		<div style="width: 30%; margin: 30px auto">
			<form action="/ramenspot" method="POST">
				<div class="form-group">
						<input class="form-control" type="text" placeholder="name" name="name">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" placeholder="image url" name="image">
				</div>
				<div class="form-group">
						<input class="form-control" type="text" placeholder="description" name="description">
				</div>
				<div class="form-group">
						<input class="form-control" type="text" placeholder="price" name="price">
				</div>
				<div class="form-group">
					<button class="btn btn-sm btn-primary btn-block">Submit
					</button>
				</div>
			</form>
			<a href="/ramenspot">Go Back</a>
		</div>

	</div>
</div>



<% include /partials/footer %>